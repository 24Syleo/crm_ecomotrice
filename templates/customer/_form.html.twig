{{ form_start(form) }}

<div class="row mb-3">
	<div class="col-sm-6">
		{{ form_row(form.name) }}
	</div>
	<div class="col-sm-6">
		{{ form_row(form.address) }}
	</div>
</div>

<div class="row mb-3">
	<div class="col-sm-6">
		{{ form_row(form.npa) }}
	</div>
	<div class="col-sm-6">
		{{ form_row(form.city) }}
	</div>
</div>

<div class="row mb-3">
	<div class="col-sm-6">
		{{ form_row(form.phone) }}
	</div>
	<div class="col-sm-6">
		{{ form_row(form.email) }}
	</div>
</div>

<div class="row mb-3">
	<div class="col-sm-6">
		{{ form_row(form.lat) }}
	</div>
	<div class="col-sm-6">
		{{ form_row(form.lng) }}
	</div>
</div>

<div class="row mb-3">
	{{ form_row(form.information) }}
</div>

{{ form_end(form) }}
